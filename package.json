{
  "name": "picto-chan",
  "description": "A 4chan project",
  "version": "1.0.0",
  "author": "Evan M Jones",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "npm run dev-client & npm run dev-server & nodemon dist/server.js",
    "dev-client": "cross-env NODE_ENV=development webpack-dev-server --config webpack.client --open --hot",
    "dev-server": "cross-env NODE_ENV=development webpack --config webpack.server --watch",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "clean": "rm -rf node_modules build dist"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "compression": "^1.7.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "image-preload": "^1.0.6",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss-cssnext": "^3.0.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6",
    "vue": "^2.4.4",
    "vue-loader": "^13.0.5",
    "vue-material": "^0.8.1",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vue-template-compiler": "^2.4.4",
    "vue2-redux": "^1.0.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
