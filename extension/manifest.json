{
  "manifest_version": 2,
  "name": "Picto Chan Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://boards.4chan.org/*/*/*/*", "*://boards.4chan.org/*/*/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png",
    "48": "icons/android-icon-48x48.png",
    "96": "icons/favicon-96x96.png"
  }
}
